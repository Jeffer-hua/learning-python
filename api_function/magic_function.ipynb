   "source": [
    "#### 过滤出1~100中平方根是整数的数\n",
    "- filter(function, iterable) 通过function过滤iterable"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "import math"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[1, 4, 9, 16, 25, 36, 49, 64, 81, 100]\n"
     ]
    }
   ],
   "source": [
    "def is_sqrt(x):\n",
    "    if math.sqrt(x) % 1 == 0 :\n",
    "        return x\n",
    "\n",
    "tmp_list=filter(is_sqrt,range(1,101))\n",
    "result=list(tmp_list)\n",
    "print(result)"
   ]
  }
 ],
